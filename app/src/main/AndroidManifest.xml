<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alanger.peruappstest">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppThemeMaterial">
        <activity
            android:parentActivityName="com.alanger.peruappstest.main.view.TabbetActivity"
            android:name="com.alanger.peruappstest.postDetail.view.PostDetailActivity">

        </activity>
        <activity
            android:name="com.alanger.peruappstest.main.view.TabbetActivity"
            android:theme="@style/AppThemeMaterial.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>